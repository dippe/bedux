{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "quotemark": false,
    "no-debugger": false,
    "only-arrow-functions": false,
    "no-unused-expression": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
